{
  "parser": "@babel/eslint-parser",
  "parserOptions": {
    "requireConfigFile": false
  },
  "env": {
    "es6": true,
    "browser": true,
    "node": true
  },
  "rules": {
    "semi": "off",
    "max-len": ["error", { "code": 90 }],
    "object-curly-spacing": "off",
    "require-jsdoc": "off",
    "linebreak-style": ["error", "windows"],
    "arrow-parens": "off"
  },
  "extends": ["eslint:recommended", "google"]
}
